{
  "name": "ğŸ‘ï¸ Eyes",
  "settings": {
    "show_variables": true
  },
  "variables": {
    "makeup_type": ["eyeliner", "eyeshadow", "mascara"],
    "makeup_color": ["{color}"],
    "eyewear_color": {
      "tags": ["{color}"],
      "suffix": " eyewear"
    },
    "eyewear_shape": {
      "probability": 0.1,
      "tags": ["rectangular", "round"],
      "suffix": " eyewear"
    }
  },
  "tags": {
    "eye_color": {
      "tags": ["{color}"],
      "suffix": " eyes"
    },
    "makeup": {
      "probability": 0.2,
      "tags": { "makeup": "{makeup_color} {makeup_type}" }
    },
    "eyewear": {
      "group_labels": true,
      "probability": 0.2,
      "distribution": [0.5, 0.3],
      "tags": {
        "regular": "glasses, {eyewear_color}, {eyewear_shape}",
        "sunglasses": "sunglasses, {eyewear_color}, {eyewear_shape}",
        "tinted_glasses": "glasses, tinted eyewear, {eyewear_color}, {eyewear_shape}",
        "eyepatch": "eyepatch, {eyewear_color}",
        "googles": ["googles on head", "googles around neck"]
      }
    },
    "eye_effect": {
      "probability": 0.1,
      "tags": { "eye_effect": ["glowing eyes", "no pupils", "sparkling eyes"] }
    }
  }
}
