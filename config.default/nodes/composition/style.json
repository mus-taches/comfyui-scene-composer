{
  "name": "ðŸŽ¨ Style",
  "tags": {
    "quality": {
      "group_labels": true,
      "tags": {
        "masterpiece": "masterpiece, best quality, absurdres",
        "masterpiece+": "masterpiece, best quality, absurdres, amazing quality, very aesthetic, newest, volumetric lighting,",
        "score_up": "score_9, score_8_up, score_7_up"
      }
    },
    "lineart": {
      "probability": 0.5,
      "number": [1, 2],
      "tags": [
        "lineart",
        "thick outlines",
        "color trace",
        "colored lineart",
        "continuous line"
      ]
    },
    "color_palette": {
      "probability": 0.2,
      "tags": [
        "monochrome",
        "flat colors",
        "partially colored",
        "pastel colors",
        "dark colors",
        "muted colors",
        "pale colors",
        "sepia",
        "saturated colors"
      ]
    },
    "color_theme": {
      "probability": 0.05,
      "tags": "{color}",
      "suffix": " theme"
    },
    "light": {
      "probability": 0.5,
      "distribution": [0.2, 0.2, 0.2, 0.2],
      "tags": [
        "dim lighting",
        "sidelighting",
        "backlighting",
        "underlighting",
        "overexposure",
        "high contrast"
      ]
    },
    "light_effect": {
      "number": [1, 3],
      "probability": 0.4,
      "tags": ["light particles", "lens flare", "bloom", "drop shadow"]
    },
    "depth of field": {
      "probability": 0.4,
      "tags": {
        "depth of field": {
          "number": [1, 2],
          "tags": ["depth of field", "bokeh", "blurry background", "bloom"]
        }
      }
    }
  },
  "output": {
    "group_labels": true,
    "prefix": "{quality},",
    "tags": {
      "lineart": "lineart,continuous line",
      "doujinshi": "{lineart},monochrome,grayscale,splash page",
      "unicolor": "monochrome,{color_palette}"
    }
  },
  "custom_output": "{quality},{lineart},{color_palette},{color_theme},{light},{light_effect}"
}
